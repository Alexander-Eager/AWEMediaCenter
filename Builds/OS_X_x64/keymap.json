		"0x02000000": {
			"_comment": "Shift"

		},
		"0x04000000": {
			"_comment": "Control (Command on OS X)"

		},
		"0x08000000": {
			"_comment": "Alt"

		},
		"0x10000000": {
			"_comment": "Meta (Control on OS X)"

		},
		"0x06000000": {
			"_comment": "Shift + Control"

		},
		"0x0A000000": {
			"_comment": "Shift + Alt"

		},
