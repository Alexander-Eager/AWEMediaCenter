			"0x01000015": ["CursorDown"],

			"0x01000016": ["ScrollPageUp"],
			"0x01000017": ["ScrollPageDown"],

			"0x01000000": ["PageBack"],
			"0x01000005": ["Select"],

			"0x01000010": ["GoHome"],

			"0x01000011": ["PlaybackStop"],
